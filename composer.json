{
    "name": "smileraj/laravelpackage",
    "description": "LaravelPackage provide a separate package with skelton of laravel",
    "keywords": ["laravel", "laravelpackage","package"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rajkumar",
            "email": "rkopenmail@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "^7.24",
        "php": "^7.2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0 || ~6.0 || ~7.0"
    },
    "autoload": {
        "psr-4": {
            "smileraj\\LaravelPackage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
           
        }
    },
    "suggest": {
        "smileraj/notify": "Simple flash notifications for Laravel"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "smileraj\\LaravelPackage\\LaravelPackageServiceProvider"
            ],
            "aliases": {
                "Cart": "smileraj\\LaravelPackage\\Facades\\Cart"
            }
        }
    }
}
